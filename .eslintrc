{
  "extends": "airbnb",
  "env": {
    "node": true,
    "jasmine": true,
    "browser": true
  },
  "rules": {
    "lines-between-class-members": 0,
    "object-curly-newline": 0,
    "max-len": 0,
    "no-param-reassign": 0 // :(
  },
  "plugins": ["babel"],
  "globals": {
    "browser": false,
    "window": true,
    "document": true
  },
  "parser": "babel-eslint"
}
